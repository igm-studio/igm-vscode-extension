{
	"name": "igm-syntax",
	"displayName": "IGM-syntax",
	"description": "Ebic extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "igm",
				"aliases": [
					"Idle Game Maker",
					"igm"
				],
				"extensions": [
					".igm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "igm",
				"scopeName": "source.igm",
				"path": "./syntaxes/igm.tmLanguage.json"
			}
		]
	}
}
